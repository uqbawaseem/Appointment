
<%= form_for @student do |f|%>
<%= f.label :Name%>
<%= f.text_field :name%><br>
<%= f.label :Age%>
<%= f.text_field :age%><br>
<%= f.label :Email%>
<%= f.text_field :email%><br>
<%= f.label :Password%>
<%= f.text_field :password%><br>
<%= f.label :Status%>
<%= f.text_field :status%>
<%= f.submit%>
<% end %>