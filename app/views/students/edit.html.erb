<%=form_for @student  do |f|   %>

  <%= f.label :Name%>
  <%= f.text_field :name%>
  <br><br>
  <%= f.label :Age%>
  <%= f.text_field :age%>
  <br><br>
  <%= f.label :Email%>
  <%= f.text_field :email%><br>
  <%= f.label :Password%>
  <%= f.text_field :password_digest%><br>
  <%= f.label :Status%>
  <%= f.text_field :status%>
  <br><br>
<br><br>
  <%= f.submit "update"%>

  <% end %>